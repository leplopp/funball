	public Screen(int width, int heigth) {
		Random random = new Random();
		super(width, heigth);
		apclix = new Render(256, 256);
		for (int i = 0; i<256*256; i++)	{
			
		}
	}
	
	public void render()	{
		draw(apclix, 0, 0);
	}

}
